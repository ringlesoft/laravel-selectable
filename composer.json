{
    "name": "ringlesoft/laravel-selectable",
    "description": "A package to populate select options from laravel collections.",
    "type": "laravel-package",
    "homepage": "https://ringlesoft.com/packages/laravel-selectable",
    "license": "MIT",
    "keywords": [
        "ringlesoft",
        "selectable",
        "collection",
        "select-options",
        "options",
        "laravel"
    ],
    "autoload": {
        "psr-4": {
            "RingleSoft\\LaravelSelectable\\": "src/"
        }
    },
    "authors": [
        {
            "name": "David Ringle",
            "email": "ringunger@gmail.com",
            "homepage": "https://ringlesoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/collections": "^9.0|^10.0|^11.0",
        "illuminate/routing": "^9.0|^10.24|^11.0"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RingleSoft\\LaravelSelectable\\LaravelSelectableServiceProvider"
            ],
            "aliases": {
                "LaravelSelectable": "Ringlesoft\\LaravelSelectable\\Facades\\Selectable"
            },
            "dont-discover": [
            ]
        }
    },
    "branches": {
        "stable": {
            "name": "master"
        }
    },
    "version": "1.0.0",
    "minimum-stability": "dev",
    "prefer-stable": true
}
